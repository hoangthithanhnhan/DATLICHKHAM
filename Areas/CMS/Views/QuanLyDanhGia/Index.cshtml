@{
    ViewBag.Title = "Quản lý đánh giá";
}
<div class="content">
    <div class="d-flex align-items-center block-header">
        <div class="search-block">
            <input id="search" type="text" placeholder="Tìm kiếm...">
            <button id="btn-search"> <img src="~/images/search-cms.png" alt=""></button>
        </div>
    </div>
    <table class="table table-border" id="myTable">
    <thead>
        <tr>
            <th>Stt</th>
            <th>Họ tên</th>
            <th>Trạng thái</th>
            <th>Số sao</th>
            <th>Ngày đánh giá</th>
            <th>Nội dung</th>
            <th>Thao tác</th>
        </tr>
    </thead>
</table>
</div>

@section Scripts {
    <script src="~/PageScripts/QuanLyDanhGia.js"></script>
}
